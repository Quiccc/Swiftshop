<head>
    <link href="https://fonts.cdnfonts.com/css/bukhari-script" rel="stylesheet">
</head>
<body>
    @*Navbar*@
    <div class="navbar fixed-top mb-1">
        <div class="w-100 d-flex align-items-center justify-content-between">
            <div class="mt-3 ms-4" style="width:30%"><a asp-controller="ShoppingList" asp-action="ActiveLists" asp-route-Page="1"><img style="width:30%" src="~/logo.png" /></a></div>
            <div class="mt-1 me-4"><partial name="_LoginPartial" /></div>
        </div>
    </div>
    @*Main Canvas*@
    <div class="container" style="margin-top:7%;">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>